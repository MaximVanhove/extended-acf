{
    "name": "vinkla/extended-acf",
    "description": "Register advanced custom fields with object oriented PHP",
    "license": "MIT",
    "keywords": [
        "acf",
        "advanced",
        "custom",
        "extended",
        "fields",
        "wordpress"
    ],
    "authors": [
        {
            "name": "Vincent Klaiber",
            "email": "hello@doubledip.se"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "symfony/var-dumper": "^6.3",
        "phpunit/phpunit": "^10.1"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Extended\\ACF\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Extended\\Tests\\ACF\\": "tests/"
        },
        "files": [
            "tests/helpers.php"
        ]
    },
    "suggest": {
        "symfony/var-dumper": "Required to use the dump method (^6.3)."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "13.5-dev"
        }
    }
}
